{
  "provisioners": [
    {  
      "type": "file",
      "source": "/var/InstallationApplicationGcloud",
      "destination": "/var"
    },
    {
      "type": "shell",
      "execute_command":"chmod +x {{ .Path }}; sudo '{{ .Path }}'; sh '{{ .Path }}'",
      "inline": [
        "bash /var/InstallationApplicationGcloud/main.sh"
      ]
    }
  ],

  "builders": [
    {
      "type": "googlecompute",
      "project_id": "projectID",
      "account_file": "accountFILE",
      "source_image": "ubuntu-1604-xenial-v20170330",
      "zone": "europe-west1-b",
      "ssh_username": "ubuntu",
      "ssh_password": "cvotest",
      "image_name": "packer-image-stage"
    }
  ]
}
